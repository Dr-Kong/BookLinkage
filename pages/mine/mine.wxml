<!--pages/mine/mine.wxml-->
<view class="container">
	<view class="userBackground">
		<image class="userInfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover"></image>
		<view class="userinfo-nickname">{{userInfo.nickName}}</view>
	</view>

	<view class="user-content">
		<button bindtap="to_upload">上传书本</button>
		
		<button class="user-history" bindtap="to_istory">
		历史记录
		</button>

		<button class="user-starred" bindtap="to_starred">
		我的收藏
		</button>

		<button class="user-feedback" bindtap="to_feedback">
		解答反馈
		</button>

		<button class="user-about" bindtap="to_about">
		关于我们
		</button>
	</view>
	
</view>
